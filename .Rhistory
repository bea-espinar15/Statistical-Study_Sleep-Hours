# T3: VARIABLES ALEATORIAS Y MODELOS DE PROBABILIDAD
# --------------------------------------------------
# NEH: variable discreta --> FUNCIÓN DE MASA
func.mas.NEH <- function(x) {
return(prob.7[x])
}
# función de distribución
func.dist.NEH <- function(x) {
sum <- 0
for (i in 1:x)
sum <- sum + func.mas.NEH(i)
return(sum)
}
